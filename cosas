  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
  <script>
  document.addEventListener('DOMContentLoaded', function () {
      // Puedes agregar aquí la lógica para mostrar o no el selector según ciertas condiciones
  });
  
  function selectLanguage(language) {
      // Guardar el idioma seleccionado en el almacenamiento local
      localStorage.setItem('selectedLanguage', language);
  
      // Redirigir según el idioma seleccionado
      if (language === 'en') {
          location.href = "eng/index.html";
      } else {
          location.href = "../index.html";
      }
  }
  </script>
  