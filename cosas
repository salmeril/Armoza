<!-- Language Selector -->
<div id="languageSelector" class="bg-light">
 
  <div class="language-option" onclick="selectLanguage('es')">
      <img src="/src/img/icons/espaicon.png" alt="Español">
      
  </div>
  <div class="language-option m-1" onclick="selectLanguage('en')">
      <img src="/src/img/icons/icon.png" alt="English">
      
